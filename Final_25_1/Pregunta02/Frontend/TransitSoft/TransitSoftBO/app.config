<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="TipoLicenciaWSPortBinding" />
                <binding name="VehiculoWSPortBinding" />
                <binding name="ConductorWSPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/TransitSoftWS/TipoLicenciaWS"
                binding="basicHttpBinding" bindingConfiguration="TipoLicenciaWSPortBinding"
                contract="TransitSoftWS.TipoLicenciaWS" name="TipoLicenciaWSPort" />
            <endpoint address="http://localhost:8080/TransitSoftWS/VehiculoWS"
                binding="basicHttpBinding" bindingConfiguration="VehiculoWSPortBinding"
                contract="TransitSoftWS.VehiculoWS" name="VehiculoWSPort" />
            <endpoint address="http://localhost:8080/TransitSoftWS/ConductorWS"
                binding="basicHttpBinding" bindingConfiguration="ConductorWSPortBinding"
                contract="TransitSoftWS.ConductorWS" name="ConductorWSPort" />
        </client>
    </system.serviceModel>
</configuration>